{
  "name": "Izumi-v3",
  "description": "Izumi a Multi Device whatsapp bot",
  "logo": "https://i.imgur.com/ZxbCSmN.jpeg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/papaigwe1234/KARLA-MD",
  "website": "https://eypz-izumi.vercel.app",
  "success_url": "https://github.com/papaigwe1234/KARLA-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "enter your session id",
      "required": "true"
    },
     "HEROKU": {
      "description": "Don't change this",
      "required": true,
       "value":"true"
     },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": "true"
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": "true"
    },
    "MENU_URL": {
      "description": "Add Url of image or Video",
      "required": "false",
      "value": "https://i.imgur.com/ZxbCSmN.jpeg"
     },
     "BOT_NAME": {
      "description": "enter your bot name",
      "required": "false",
      "value": "KARLA-ᴍᴅ"
    },
       "STICKER_PACKNAME": {
      "description": "enter tou sticker pack name",
      "required": "false",
      "value": "karla-ᴍᴅ"
    }, "AUDIO_DATA": {
      "description": "enter your audio data(title,author,link)",
      "required": "false",
      "value": "enjoy this song,Iᴢᴜᴍɪ-v3,https://i.imgur.com/cO0TZJv.jpeg"
    },
       "OWNER_NAME": {
       "description": "Enter your name",
       "required": "false",
       "value": "igwe"
    },
       "CAPTION": {
       "description": "Enter youR Caption",
       "required": "false"
       "value" : ""
    },
        "PREFIX": {
       "description": "prefix for bot",
       "required": "false",
       "value": "."
    },
     "AUTHOR": {
      "description": "enter sticker pack author name ",
      "required": "false",
      "value": "igwetech"
    },
    "SUDO": {
      "description": "add Sudo number",
      "required": "false",
      "value": "24160338758"
    },
    "MODE": {
      "description": "bot work type",
      "required": "true",
      "value": "private"
     },
 "addons": [{
      "plan": "heroku-postgresql"
    }],
    "buildpacks": [{
      "url": "heroku-community/apt"
      },
      {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
      }]
  }
